<div class="forms">
<%= bootstrap_form_for @video, html: { multipart: true } do |f| %>
    <%= f.text_field :title %>
  <br>
    <%= f.text_field :user %>
  <br>
    <%= f.text_field :embed_url %>
  <br>
    <%= f.text_field :thumbnail_url %>
  <br>
    <%= f.text_field :dncrew %>
  <br>
    <%= f.text_field  :series %>
  <br>
    <%= f.text_field :description %>
  <br>
  <%= f.submit "Create!" %>
<% end %>

<ul>
<% @video.errors.full_messages.each do |message| %>
  <li><%= message %></li>
<% end %>
</ul>
</div>
